{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "compiler": "ttypescript"
    }
  },
  "setupFiles": [
    "<rootDir>/../mock.config.ts"
  ]
}
