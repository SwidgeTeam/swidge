{
   "name": "backend",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "rm -rf dist && nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest --config test/unit/jest-unit.json",
      "test:watch": "jest --watch --config test/unit/jest-unit.json",
      "test:cov": "jest --coverage --config test/unit/jest-unit.json",
      "test:debug": "node --config test/unit/jest-unit.json --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config test/e2e/jest-e2e.json",
      "ormconfig": "(rm ormconfig.json 2>/dev/null || true) && ts-node -r tsconfig-paths/register scripts/write-ormconfig.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migration:generate": "yarn ormconfig && yarn build && yarn run typeorm migration:generate -n default",
      "migration:create": "yarn run typeorm migration:create -n",
      "migration:run": "yarn ormconfig && yarn run typeorm migration:run",
      "migration:revert": "yarn ormconfig && yarn run typeorm migration:revert"
   },
   "dependencies": {
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^2.0.0",
      "@nestjs/core": "^8.0.0",
      "@nestjs/cqrs": "^8.0.2",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/typeorm": "^8.0.3",
      "@sushiswap/sdk": "^5.0.0-canary.116",
      "axios": "^0.26.1",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "dotenv": "^16.0.0",
      "ethers": "^5.6.2",
      "mysql": "^2.14.1",
      "mysql2": "^2.3.3",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0",
      "typeorm": "0.2.43"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "27.4.0",
      "@types/node": "^16.11.10",
      "@types/sinon": "^10.0.11",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "^27.5.1",
      "jest-ts-auto-mock": "^2.1.0",
      "prettier": "^2.3.2",
      "sinon": "^13.0.1",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-auto-mock": "^3.5.0",
      "ts-jest": "^27.1.4",
      "ts-loader": "^9.2.3",
      "ts-node": "10.4.0",
      "tsconfig-paths": "^3.10.1",
      "ttypescript": "^1.5.13",
      "typescript": "^4.6.3"
   }
}
